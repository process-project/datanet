<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:with type="pl.cyfronet.datanet.web.client.widgets.repositorypanel.RepositoryPanelMessages" field="messages"/>
	<ui:style type="pl.cyfronet.datanet.web.client.widgets.repositorypanel.RepositoryPanelWidget.RepositoryPanelWidgetStyles">
		.left {
			float: left;
			display: block;
		}
	</ui:style>
	<g:DockLayoutPanel>
		<g:north size="40">
			<g:FlowPanel>
				<b:Button type="INFO" href="" target="_blank" ui:field="repositoryLink"/>
			</g:FlowPanel>
		</g:north>
		<g:center>
			<b:TabPanel tabPosition="left" ui:field="tabPanel"></b:TabPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>