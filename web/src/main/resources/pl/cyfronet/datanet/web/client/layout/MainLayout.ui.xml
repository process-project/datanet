<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DockLayoutPanel>
	    <g:north size="50">
	        <g:FlowPanel ui:field="header"/>
		</g:north>
		<g:center>
		    <g:SplitLayoutPanel>
		        <g:west size="300">
		            <g:FlowPanel ui:field="west"/>
		        </g:west>
		        <g:center>
		            <g:FlowPanel ui:field="center"/>
		        </g:center>
		    </g:SplitLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 